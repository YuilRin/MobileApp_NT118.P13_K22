<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dn_mobile_navigation"
    app:startDestination="@id/homeDnFragment">

    <fragment
        android:id="@+id/homeDnFragment"
        android:name="com.example.mobileapp.uidn.home.HomeDnFragment"
        android:label="fragment_home_dn"
        tools:layout="@layout/business_fragment_home" >

        <action
            android:id="@+id/action_homeDnFragment_to_donHangFragment"
            app:destination="@id/donHangFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_sanPhamFragment"
            app:destination="@id/sanPhamFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_baoCaoFragment"
            app:destination="@id/baoCaoFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_nhaCungCapFragment"
            app:destination="@id/nhaCungCapFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_khoHangFragment"
            app:destination="@id/khoHangFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_nhanVienFragment"
            app:destination="@id/nhanVienFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_thongBaoFragment"
            app:destination="@id/thongBaoFragment" />
        <action
            android:id="@+id/action_homeDnFragment_to_caiDatFragment"
            app:destination="@id/caiDatFragment" />
    </fragment>
    <fragment
        android:id="@+id/moreFragment"
        android:name="com.example.mobileapp.uidn.more.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/business_fragment_more" />
    <fragment
        android:id="@+id/quanLyFragment"
        android:name="com.example.mobileapp.uidn.QuanLy.QuanLyFragment"
        android:label="fragment_quan_ly"
        tools:layout="@layout/business_fragment_quanly" />
    <fragment
        android:id="@+id/donHangFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.DonHangFragment"
        android:label="fragment_don_hang"
        >
    <action
        android:id="@+id/action_donHangFragment_to_homeDnFragment"
        app:destination="@id/homeDnFragment" />
    </fragment>
    <fragment
        android:id="@+id/sanPhamFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.SanPhamFragment"
        android:label="SanPhamFragment" />
    <fragment
        android:id="@+id/baoCaoFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.BaoCaoFragment"
        android:label="BaoCaoFragment" />
    <fragment
        android:id="@+id/caiDatFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.CaiDatFragment"
        android:label="CaiDatFragment" />
    <fragment
        android:id="@+id/khoHangFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.KhoHangFragment"
        android:label="KhoHangFragment" >
        <action
            android:id="@+id/action_khoHangFragment_to_storageFragment"
            app:destination="@id/storageFragment" />
    </fragment>
    <fragment
        android:id="@+id/nhaCungCapFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.NhaCungCapFragment"
        android:label="NhaCungCapFragment" />
    <fragment
        android:id="@+id/nhanVienFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.NhanVienFragment"
        android:label="NhanVienFragment" />
    <fragment
        android:id="@+id/thongBaoFragment"
        android:name="com.example.mobileapp.uidn.FragmentButton.ThongBaoFragment"
        android:label="ThongBaoFragment" />
    <fragment
        android:id="@+id/storageFragment"
        android:name="com.example.mobileapp.uidn.TabLayoutFragment.Fragment.StorageFragment"
        android:label="StorageFragment" />

</navigation>